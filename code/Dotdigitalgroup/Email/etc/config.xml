                        <class>ddg_automation/sales_observer</class>
                        <method>handleSalesOrderPlaceAfter</method>
                    </ddg_record_sale>
                </observers>
            </sales_order_place_after>
            <sales_order_save_after>
                <observers>
                    <ddg_checkout_type_onepage_save_order_after>
