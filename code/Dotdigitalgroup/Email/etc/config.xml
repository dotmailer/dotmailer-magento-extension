                <observers>
                    <ddg_sales_model_service_quote_submit_success>
                        <type>singleton</type>
                        <class>ddg_automation/customer_observer</class>
                        <method>handleQuoteSubmitSuccess</method>
                    </ddg_sales_model_service_quote_submit_success>
                </observers>
